<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/white"
    tools:context="com.fishe.wut2dodemo.Genre">


    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView"
        android:layout_toEndOf="@+id/imageView"
        android:layout_toRightOf="@+id/imageView"/>

    <ImageView
        android:layout_width="25dp"
        android:layout_height="42dp"
        android:id="@+id/imageView"
        android:src="@drawable/game"
        android:layout_alignTop="@+id/listView"
        android:layout_marginTop="5dp"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:src="@drawable/ic_nature_black_24dp"
        android:layout_marginTop="19dp"
        android:layout_below="@+id/imageView4"
        android:layout_toStartOf="@+id/listView" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:src="@drawable/ic_account_balance_black_24dp"
        android:layout_marginTop="10dp"
        android:layout_toStartOf="@+id/listView"
        android:layout_below="@+id/imageView" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView4"
        android:layout_marginTop="65dp"
        android:src="@drawable/music"
        android:layout_below="@+id/imageView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView6"
        android:layout_marginTop="19dp"
        android:src="@drawable/movie"
        android:layout_below="@+id/imageView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView7"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_extension_black_24dp"
        android:layout_below="@+id/imageView2"
        android:layout_toStartOf="@+id/listView" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView8"
        android:layout_marginTop="25dp"
        android:src="@drawable/ic_directions_bike_black_24dp"
        android:layout_below="@+id/imageView7"
        android:layout_toStartOf="@+id/listView" />

</RelativeLayout>
